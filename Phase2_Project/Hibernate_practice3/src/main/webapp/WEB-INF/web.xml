<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Product</servlet-name>
  	<display-name>Product</display-name>
  	<description></description>
  	<servlet-class>com.entity.Product</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveObject</servlet-name>
  	<display-name>SaveObject</display-name>
  	<description></description>
  	<servlet-class>com.test.SaveObject</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Utiltity</servlet-name>
  	<display-name>Utiltity</display-name>
  	<description></description>
  	<servlet-class>com.utility.Utiltity</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Product</servlet-name>
  	<url-pattern>/Product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveObject</servlet-name>
  	<url-pattern>/SaveObject</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Utiltity</servlet-name>
  	<url-pattern>/Utiltity</url-pattern>
  </servlet-mapping>
</web-app>
